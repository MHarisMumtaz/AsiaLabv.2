</div>
<br /><br /><br />

<script>
    function printSummary() {
        var summarydate = $('#summaryDate').val();
        var branchId = $('#BranchId').val();
        var model={
            Date : summarydate,
            BranchId:branchId
        }
        $.ajax({
            type: 'POST',
            url: "@Url.Action("PrintSummary","Admin")",
            data: JSON.stringify(model),
            contentType: "application/json",
            dataType: 'json',
            success: function (data) {
                alert(data);
        }
    });
    }
</script>



