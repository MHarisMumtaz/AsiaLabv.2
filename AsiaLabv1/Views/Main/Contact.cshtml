@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_SecondMainLayout.cshtml";
}

@section Title
{
    Contact Us
}
@section Banner
{
    <br><br><div class="page-head">
    <div class="container">
        <h3>Contact</h3>
    </div>
</div>

}
<!-- contact -->
<div class="contact">
    <div class="container">
        <div class="contact-grids">
            <div class="map wow fadeInDown animated" data-wow-delay=".5s">
                <h3 class="tittle">View On Map</h3>
                <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14467.95696234362!2d67.0673723!3d24.9664807!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x16a69cf0d9b3b235!2sAsia+Lab+%26+Diagnostic+Centre!5e0!3m2!1sen!2s!4v1462431945748" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
            </div>


            <div class="col-md-4 contact-grid text-center animated wow slideInLeft" data-wow-delay=".5s">
                <div class="contact-grid1">

                    <i class="glyphicon glyphicon-map-marker" aria-hidden="true"></i>
                    <h4>Address</h4>
                    <p>Shahrah-e-Usman, Sector 11H<span>Karachi, Sindh.</span></p>
                </div>
            </div>
            <div class="col-md-4 contact-grid text-center animated wow slideInLeft" data-wow-delay=".5s">
                <div class="contact-grid1">

                    <i class="glyphicon glyphicon-map-marker" aria-hidden="true"></i>
                    <h4>Call Us</h4>
                    <p>+92-333-2277216</p>
                </div>
            </div>

            <div class="col-md-4 contact-grid text-center animated wow slideInRight" data-wow-delay=".5s">
                <div class="contact-grid3">
                    <i class="glyphicon glyphicon-envelope" aria-hidden="true"></i>
                    <h4>Email</h4>
                    <p><a href="mailto:info@example.com">info@example1.com</a><span><a href="mailto:info@example.com">info@example2.com</a></span></p>
                </div>
            </div>
            <div class="clearfix"> </div>
        </div>


    </div>
</div>



